<% layout('layout/page') -%>
<% block('title', "Paint") %>

<html lang="en" ng-app="myApp" class="no-js">
<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="description" content="">
<meta name="viewport" content="width=device-width, initial-scale=1">

<body ng-controller="View1Controller">

<script src="angular/angular.js"></script>
<script src="angular-route/angular-route.js"></script>
<script src="view1/view1.js"></script>
<script src="view1/image-service.js"></script>
<script src="view1/imagelist.js"></script>
<script src="view1/fabric.js"></script>
<script src="view1/canvas-directive.js"></script>
<script src="view1/drop-directive.js"></script>
<script src="version/version.js"></script>
<script src="version/version-directive.js"></script>
<script src="version/interpolate-filter.js"></script>

<script src="/view1/appPaint.js"></script>

<canvas id='c' canvas-directive></canvas>
<div ng-controller="ImageListController">
    <div style="min-height: 200px; border: 1px solid black;" drop-directive>
        <button ng-click="Inc(5)">Кисть 1</button>
        <button ng-click="Inc(15)">Кисть 2</button>
        <button ng-click="Inc(30)">Кисть 3</button>

        <li ng-repeat="image in images">
            <image-directive></image-directive>
        </li>
    </div>
</div>
<button ng-click="send()">Отправить</button>



<!--<button id="init" ng-click="init_image()" class="btn btn-info">Init image</button>-->
<br>
<div style="display: inline-block; margin-left: 10px">
    <button id="clear-canvas" class="btn btn-info" ng-click="Clear()">Clear</button>
    <br>
    <div id="drawing-mode-options">
        <label for="drawing-color">Line color:</label>
        <input type="color" value="#005E7A" id="drawing-color" ng-model="brushcolor">
    </div>
</div>
